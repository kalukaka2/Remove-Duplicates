class Solution {
    public int removeDuplicates(int[] n) {
        int ans=1;
        int index=1;
        for(int i=0;i<n.length-1;i++){
            if(n[i]!=n[i+1]){
                ans++;
                n[index]=n[i+1];
                index++;
            }
        }
        return ans;
    }
}
